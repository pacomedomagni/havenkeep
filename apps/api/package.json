{
  "name": "havenkeep-api",
  "version": "1.0.0",
  "description": "HavenKeep REST API - Express + PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "node-pg-migrate up",
    "db:migrate:down": "node-pg-migrate down",
    "db:seed": "tsx src/db/seed.ts",
    "test": "jest"
  },
  "keywords": ["havenkeep", "api", "express", "postgresql"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.12.0",
    "express-rate-limit": "^7.1.5",
    "redis": "^4.6.12",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1540.0",
    "sharp": "^0.33.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "dotenv": "^16.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^14.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-google-oauth20": "^2.0.14",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "node-pg-migrate": "^6.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
